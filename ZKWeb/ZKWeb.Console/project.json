{
	"version": "1.1.1-beta-3",
    "dependencies": {
        "NETStandard.Library": "1.6.0",
        "Newtonsoft.Json": "9.0.1",
        "NSubstitute": "2.0.0-rc",
        "ZKWeb": "1.1.1-beta-3",
        "ZKWeb.ORM.Dapper": "1.1.1-beta-3",
        "ZKWeb.ORM.EFCore": "1.1.1-beta-3",
        "ZKWeb.ORM.InMemory": "1.1.1-beta-3",
        "ZKWeb.ORM.MongoDB": "1.1.1-beta-3",
        "ZKWeb.System.Drawing": "2.0.0-beta-1",
        "ZKWebStandard": "1.1.1-beta-3"
